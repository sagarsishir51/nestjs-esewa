{
  "name": "@dallotech/nestjs-esewa",
  "version": "0.0.6",
  "description": "This is simple wrapper for Esewa Payment. It supports Epay-V2 and transaction verification for Esewa SDK, but later more will be added. Just ping us or open pull request and contribute :)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest --detectOpenHandles",
    "prepublish": "npm run build"
  },
  "keywords": [
    "nestjs",
    "esewa",
    "api",
    "dallotech"
  ],
  "author": "Dallo Tech Pvt. Ltd.",
  "license": "MIT",
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "crypto-js": "^4.1.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/common": "^10.2.7",
    "@nestjs/testing": "^10.3.8",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.8.6",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
